package templates;

import migrator.Input;
import mjson.Json;

public class User {

	static String FILE_PATH = "/patient_files/patients";
	static String ID_KEY    = "id";
	static String NAME_KEY  = "name";
	
	public static Input template() {
		
		return new Input(FILE_PATH) {
			
			@Override
			public String template(Json person) {
				
				return "insert $person isa person"
						+ ", has person-id " + person.at(ID_KEY) + 
						  ", has person-name " + person.at(NAME_KEY) + ";";
			}
		};
		
	} 
}
