package templates;

import migrator.Input;
import mjson.Json;

public class Symptom {
	
	static String FILE_PATH = "/src/main/resources/patient-data-symptoms";
	static String ID_KEY   = "id";
	static String NAME_KEY = "name";

	public static Input template() {
		
		
		
		return new Input("") {
			
			@Override
			public String template(Json sympton) {
				
				return "insert $sympton isa sympton"
						+ "has symptom-id " + sympton.at(ID_KEY) + 
						  "has symptom-name " + sympton.at(NAME_KEY);
			}
		};
	} 
}
